
<div class="container">
<% @events.each do |event| %>
  <ul>
    <li>Event Name: <%= event.name %></li><br>
    <ul>
      <li>Category: <%= event.category %></li><br>
      <li>Price: $<%= event.price %></li><br>
      <li>Starts at: <%= event.event_start.strftime("%c") %></li>
    </ul>
  </ul>

<%end%>

</div>