  
<h3><%= @user.name %><br>
<%= @user.cohort %></h3>
<%= image_tag @user.avatar_url %><br>
<%= @user.email %><br>
<ul class="tabs">
  <li class="active"><a href="#tab1">What am I doing?</a></li>
  <li><a href="#tab2">What am I?</a></li>
  <!-- <li><a href="#tab3">Created Events</a></li> -->
</ul>

<div class="tab" id="tab1">
  <table>
    <% @attended_events.each do |event| %>
      <tr>
        <td> <%= link_to event.name, event %> </td>
      </tr>
    <% end %>
  </table>
</div>

<div class="tab" id="tab2">
  <table>
    <% @created_events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= link_to 'Edit', edit_user_event_path(@user.id, event) %></td>
        <td><%= button_to "delete", {:controller => :events,
      :action => 'destroy', :id => event.id, :user_id => @user.id }, :method => :delete %></td>
      </tr>
    <% end %>
  </table>
</div>
